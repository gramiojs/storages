{
	"name": "@gramio/storage-redis",
	"version": "1.0.4",
	"description": "Redis Storage for GramIO",
	"main": "./dist/ioredis.cjs",
	"module": "./dist/ioredis.js",
	"types": "./dist/ioredis.d.ts",
	"exports": {
		".": {
			"bun": {
				"types": "./dist/bun.d.ts",
				"default": "./dist/bun.js"
			},
			"import": {
				"types": "./dist/ioredis.d.ts",
				"default": "./dist/ioredis.js"
			},
			"require": {
				"types": "./dist/ioredis.d.cts",
				"default": "./dist/ioredis.cjs"
			}
		}
	},
	"type": "module",
	"homepage": "https://github.com/gramiojs/storages",
	"readme": "https://gramio.dev/storages",
	"scripts": {
		"prepublishOnly": "bunx tsc && bun test && bun ./scripts/release-jsr.ts && bunx pkgroll"
	},
	"keywords": ["gramio", "gramio-storage", "storage", "redis", "ioredis"],
	"license": "MIT",
	"devDependencies": {
		"@types/bun": "^1.2.15",
		"ioredis-mock": "^8.9.0",
		"pkgroll": "^2.12.1",
		"typescript": "^5.8.2",
		"ioredis": "^5.6.1"
	},
	"files": ["dist"],
	"dependencies": {
		"@gramio/storage": "^1.0.0"
	},
	"peerDependencies": {
		"ioredis": "^5.6.1"
	},
	"peerDependenciesMeta": {
		"ioredis": {
			"optional": true
		}
	}
}
