{
	"name": "@gramio/storage-cloudflare",
	"version": "0.0.1",
	"description": "Cloudflare KV Storage for GramIO",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		}
	},
	"type": "module",
	"homepage": "https://github.com/gramiojs/storages",
	"readme": "https://gramio.dev/storages",
	"scripts": {
		"prepublishOnly": "bunx tsc && bun test && bun ./scripts/release-jsr.ts && bunx pkgroll"
	},
	"keywords": [
		"gramio",
		"gramio-storage",
		"storage",
		"cloudflare",
		"cloudflare-storage",
		"cloudflare-kv",
		"cloudflare-kv-storage"
	],
	"license": "MIT",
	"devDependencies": {
		"@types/bun": "^1.2.8",
		"pkgroll": "^2.12.1",
		"typescript": "^5.8.2"
	},
	"files": ["dist"],
	"dependencies": {
		"@cloudflare/workers-types": "^4.20250524.0",
		"@gramio/storage": "^1.0.0"
	}
}
